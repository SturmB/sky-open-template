<?xml version='1.0' encoding='UTF-8'?>
<!--suppress XmlUnusedNamespaceDeclaration, XmlUnboundNsPrefix -->
<ExtensionManifest ExtensionBundleId="com.skyunlimitedinc.open-template" ExtensionBundleVersion="1.0.0" Version="7.0"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ExtensionList>
        <Extension Id="com.skyunlimitedinc.open-template.panel" Version="2018.0.1"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="ILST" Version="[21.0,23.9]"/>
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="7.0"/>
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.skyunlimitedinc.open-template.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./dom_html/src/index.html</MainPath>
                    <ScriptPath>./dom_app/src/app.js</ScriptPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--mixed-context</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Open Template</Menu>
                    <Geometry>
                        <Size>
                            <Width>350</Width>
                            <Height>480</Height>
                        </Size>
                        <MaxSize>
                            <Width>1200</Width>
                            <Height>1200</Height>
                        </MaxSize>
                        <MinSize>
                            <Width>240</Width>
                            <Height>200</Height>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./dom_html/src/img/icon-open-template-mediumlight.png</Icon>
                        <Icon Type="RollOver">./dom_html/src/img/icon-open-template-mediumlight.png</Icon>
                        <Icon Type="DarkNormal">./dom_html/src/img/icon-open-template-dark.png</Icon>
                        <Icon Type="DarkRollOver">./dom_html/src/img/icon-open-template-dark.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
